@model Freelance.Core.Models.ApplicationUser

<div class="job_details_area" style="margin-top: 5%; ">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-md-3"></div>
            <div class="col-lg-6 col-md-6" style="border: 1px solid #00D363; background: white;padding: 25px;">
                <div style="text-align: center;">
                    <img src="@Url.Content(Model.PhotoPath)" style="border-radius: 20%; width: 110px; height: 110px">
                    <p>@Model.UserName</p>
                </div>
                <div style="margin-top: 2%;">
                    <ul>

                        <li>@Model.FirstName</li>
                        <li>@Model.LastName</li>
                        <li>@Model.Email</li>
                        <li>@Model.PhoneNumber</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>